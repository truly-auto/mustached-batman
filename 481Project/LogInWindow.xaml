<Window x:Class="_481Project.LogInWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="LogInWindow" Height="376" Width="572" ResizeMode="NoResize">
    <Grid>
        <Button Content="Login" Height="23" HorizontalAlignment="Left" Margin="361,274,0,0" Name="confirmButton" VerticalAlignment="Top" Width="75" Click="confirmButton_Click" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Left" Margin="463,274,0,0" Name="cancelButton" VerticalAlignment="Top" Width="75" Click="cancelButton_Click" />
        <TextBox Height="27" HorizontalAlignment="Left" Margin="177,147,0,0" Name="textBox1" VerticalAlignment="Top" Width="259" />
        <TextBox Height="27" HorizontalAlignment="Left" Margin="177,202,0,0" Name="textBox2" VerticalAlignment="Top" Width="259" />
        <Label Content="Username" Height="28" HorizontalAlignment="Left" Margin="103,147,0,0" Name="label1" VerticalAlignment="Top" />
        <Label Content="Password" Height="28" HorizontalAlignment="Left" Margin="106,202,0,0" Name="label2" VerticalAlignment="Top" />
    </Grid>
</Window>
